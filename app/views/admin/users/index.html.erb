
<table border= "1" style="width:50%">
<thead>

<tr>
<th>id</th>
<th>Name</th>
<th> Last Name</th>
<th> Gendr</th>
<th> Email</th>
<th>Actions</th>
</tr>
</thead>

<tbody>
<%@users.each do |p| %>
<tr>
<td><%= p.id%></td>
<td><%= p.first_name%></td>
<td><%= p.last_name%></td>
<td><%= p.gender%></td>
<td><%= p.email%></td>
<td><%= link_to 'show', admin_user_path(p) %>|
	<%= link_to 'Destroy', admin_user_path(p), method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
<% end %>
</tbody>
</table>

